{
  "name": "fastify-gen",
  "version": "1.0.0",
  "description": "A scaffolding tool for quickly generating Fastify projects.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/hermangoncalves/fastify-gen.git"
  },
  "keywords": [
    "fastify",
    "scaffold",
    "generator",
    "cli",
    "nodejs",
    "project",
    "template"
  ],
  "author": "Herman Goncalves <hermangoncalves@outlook.com>",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js",
    "build": "pnpm -r --filter='./packages/*' run build",
    "web:preview": "pnpm --filter=fastify-gen-web run preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "packageManager": "pnpm@10.12.1"
}